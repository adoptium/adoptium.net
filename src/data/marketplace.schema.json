{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "icon": {
        "type": "string"
      },
      "iconPadding": {
        "type": "string"
      },
      "postDownload": {
        "anyOf": [{ "type": "string" }, { "type": "null" }]
      }
    },
    "required": ["name", "icon", "postDownload"],
    "additionalProperties": false
  }
}
