{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "summary": {
        "type": ["string", "null"]
      },
      "twitter": {
        "type": ["string", "null"]
      },
      "github": {
        "type": "string"
      },
      "linkedin": {
        "type": ["string", "null"]
      }
    },
    "required": ["name", "github"],
    "additionalProperties": false
  }
}
